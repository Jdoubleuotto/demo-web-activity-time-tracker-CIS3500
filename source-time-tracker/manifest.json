{
    "name": "Hello Extensions",
    "description": "Base Level Extension with Time Tracking",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "dog.png"
    },
    "icons": {
      "16": "16x16.png",
      "32": "32x32.png",
      "48": "48x48.png",
      "128": "128x128.png"
    },
    "offline_enabled": true
    
  }
  