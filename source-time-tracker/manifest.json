{
  "manifest_version": 3,
  "name": "URL Tracker",
  "version": "1.0",
  "description": "Collect and display visited URLs",
  "permissions": ["tabs", "storage", "activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "16x16.png",
      "48": "48x48.png",
      "128": "128x1288.png"
    }
  },
  "icons": {
     "16": "16x16.png",
     "48": "48x48.png",
     "128": "128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
